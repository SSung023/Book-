#include <iostream>
#include <string>
#include <vector>

using namespace std;


int main() {

    vector<int> v;

    v.push_back(10);
    v.push_back(20);
    v.push_back(30);
    v.push_back(40);
    v.push_back(50);

    vector<int>::iterator iter;

    iter = v.begin();

    cout << iter[0] << endl; //그저 접근하는 것
    cout << iter[1] << endl;

    iter += 4; //iter를 4만큼 이동시킨 것

    cout << *iter << endl;

    return 0;
}
