#include <iostream>
#include <string>
#include <vector>
#include <algorithm>

using namespace std;


int main() {

    vector<int> v(5, 0); //기본값 0으로 초기화된 size가 5인 컨테이너

    v[0] = 10;
    v[1] = 20;
    v[2] = 30;
    v[3] = 40;
    v[4] = 50;

    for(int i=0; i<v.size(); i++){
        cout << v[i] << " ";
    }
    cout << endl;

    v.resize(10); //기본값 0으로 초기화된 size가 10인 컨테이너로 확장
    for(int i=0; i<v.size(); i++){
        cout << v[i] << " ";
    }


    return 0;
}
