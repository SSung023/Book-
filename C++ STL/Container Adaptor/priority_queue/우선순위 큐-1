#include <iostream>
#include <string>
#include <algorithm>
#include <stdio.h>
#include <vector>
#include <queue>
#include <list>

using namespace std;

int main() {

    priority_queue<int> pq1;
    pq1.push(40);
    pq1.push(20);
    pq1.push(30);
    pq1.push(50);
    pq1.push(10);

    //우선 순위 큐의 기본 정렬 기준은 내림차순
    cout << "priority queue[less]" <<endl;
    while(!pq1.empty()){
        cout << pq1.top() << " ";
        pq1.pop();
    }
    cout << endl << "=============" << endl;
    


    return 0;
}
